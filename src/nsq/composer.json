{
    "name": "hyperf/nsq",
    "description": "A nsq library for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "nsq"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "hyperf/code-parser": "~3.1.0",
        "hyperf/codec": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/engine": "^2.0",
        "hyperf/support": "~3.1.0",
        "hyperf/utils": "~3.1.0",
        "jean85/pretty-package-versions": "^1.2|^2.0"
    },
    "suggest": {
        "hyperf/guzzle": "Required to use coroutine nsqd http api. (~2.2.0)"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Nsq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Nsq\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Nsq\\ConfigProvider"
        }
    }
}
